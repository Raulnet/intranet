{% extends 'base.html.twig' %}

{% block body %}
    <h1>WeezeventApiLog</h1>

    <table>
        <tbody>
            <tr>
                <th>Apiusername</th>
                <td>{{ weezeventApiLog.apiUsername }}</td>
            </tr>
            <tr>
                <th>Apipassword</th>
                <td>{{ weezeventApiLog.apiPassword }}</td>
            </tr>
            <tr>
                <th>Apikey</th>
                <td>{{ weezeventApiLog.apiKey }}</td>
            </tr>
            <tr>
                <th>Dateregister</th>
                <td>{% if weezeventApiLog.dateRegister %}{{ weezeventApiLog.dateRegister|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ weezeventApiLog.id }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('weezeventapilog_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('weezeventapilog_edit', { 'id': weezeventApiLog.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
